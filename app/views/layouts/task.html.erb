<!DOCTYPE html>
<html>
<head>
  <title>SwarmControl Tasks</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= render "shared/analyticsFeedback"%>

    <%= render "shared/navbar" %>

    <div class="container">
        <div class="row-fluid">
            <div class="span4">
                <%= yield :sidebar%>
            </div>
            <div class="span8", align="center">
                <%= yield :workspace %>
            </div>
        </div>
    </div>

    <%= render "shared/termsAndConsent"%>
    <%= render "shared/footer" %>




</body>
</html>
