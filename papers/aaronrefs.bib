	Journal = {Philosophical Magazine},
	Month = dec,
	Number = {36},
	Pages = {5685--5696},
	Title = {Rolling Resistance Moment of Microspheres on Surfaces: Contact Measurements},
	Volume = {87},
	Year = {2007},
	Bdsk-File-1 = {}}

