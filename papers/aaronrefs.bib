@inproceedings{McLurkin2013,
	Abstract = {This paper presents the r-one robot: a new, advanced, low-cost, open-source robot design that enables multi-robot research with large populations, innovative new curriculum, and unique outreach activities.  The robot has a large sensor suite and many capabilities, but a kit cost of only around \$250 per robot.  This paper describes the release version of the robot hardware, the collective user interface that allows efficient operation, and two years of experience using the platform for research, teaching, and outreach.  Novel features of the robot design are molded plastic parts, an integrated bump sensor, high-precision quadrature encoders, and a flexible inter-robot communication and localization system.  The collective user interface supports an embedded human collective interface, gang charging and autonomous docking, ground truth localization, and centralized data collection.  The advanced design, low-cost, and small size can support university research with large populations of robots, exciting multi-robot curriculum in computer science, electrical engineering, and mechanical engineering.  We conclude with tales of joy and disaster from our experience using this platform in an introductory engineering class, graduate robotics class, and several outreach presentations.},
	Author = {James McLurkin and Alvin Chou and William Li and Meagan John and Sunny Kim and Jennifer Shen and Nelson Chen and Chris Licato and Nnena Okeke and Joshua Rykowski and Quillan Kaseman and Lindsay Langford and Jeremy Hunt},
	Booktitle = {IEEE Int. Conf. Rob. Autom.},
	Date-Added = {2013-12-05 19:08:58 +0000},
	Date-Modified = {2014-01-10 02:25:49 +0000},
	Month = may,
	Title = {The r-one: An Advanced, Low-Cost Multi-Robot System for Research, Education, and Outreach},
	Year = {2013. rejected}}

@article{Y2013,
	Abstract = {Ensembles of magnetic particles are known to align and aggregate into multi-particle clusters in an applied magnetic field, and the physical laws governing these processes are well described in literature. However, it has been elusive how to achieve the opposite process, i.e. the disaggregation of particle clusters in a magnetic field. We report a novel method to disaggregate clusters of superparamagnetic microparticles using time-dependent magnetic fields. The disaggregating field is designed to generate repulsive dipole-dipole forces between the particles and to stabilize the disaggregated particles on a physical surface. We demonstrate the disaggregation of large clusters of several tens of particles, within about one minute, using fields generated by a multipole electromagnet. After the disaggregation process the particles are uniformly distributed over the surface and ready for further lab-on-chip processing. Our results represent a novel methodology to disaggregate magnetic particle clusters and thereby improve the effectiveness and reproducibility of biological assays based on magnetic microparticles.},
	Author = {Gao Y and van Reenen A and Hulsen MA and de Jong AM and Prins MW and den Toonder JM},
	Date-Added = {2013-12-05 19:08:58 +0000},
	Date-Modified = {2013-12-05 19:08:59 +0000},
	Doi = {10.1039/c3lc41229f},
	Journal = {Lab Chip},
	Month = apr,
	Number = {7},
	Pages = {1394--1401},
	Title = {Disaggregation of microparticle clusters by induced magnetic dipole-dipole repulsion near a surface.},
	Volume = {13},
	Year = {2013},
	Bdsk-File-1 = {}}

